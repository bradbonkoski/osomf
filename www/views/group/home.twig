{% extends "layout.twig" %}

{% block title %} {{ title }} {% endblock %}

{% block content %}
<h2 class="pageHeading">{{ title }}</h2>

    <table class="baseTable" id="groupHome">
        <tr>
            <th>Group Id</th><th>Group name</th><th>Group Desc</th>
            <th>Group phone</th><th>group Pager</th>
        </tr>
        {% for g in groups %}
            <tr>
                <td><a href="/osomf/group/view/{{ g.ugid }}">{{ g.ugid }}</a></td>
                <td>{{ g.groupName}}</td>
                <td>{{ g.groupDesc }}</td>
                <td>{{ g.phone }}</td>
                <td>{{ g.pager }}</td>
            </tr>
        {% endfor %}
    </table>
{% endblock %}